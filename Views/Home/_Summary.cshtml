	<td><a class="btn-link" asp-action="FoodItemDetails" asp-route-id="@Model.Id"><font color="gray">@Model.Id</font></a></td>
	<td>@Model.Name</td>
	<td>@Model.Favorites</td>
	<td>@Model.Protein</td>
	<td>@Model.Fat</td>
	<td>@Model.Carbohydrates</td>
</tr>
