<tr>
	<td>
		<a class="btn btn-default btn-sm" asp-page="/FoodItems/UpdateFoodItem" asp-route-id="@Model.Id">Update</a>
		<!--a class="btn btn-warning btn-sm" asp-action="Update2FoodItem" asp-route-id="@Model.Id">Update</a-->
